{
  "rewrites": [
    {
      "source": "/__/auth/:match*",
      "destination": "https://villa-camping-2cf57.firebaseapp.com/__/auth/:match*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        }
      ]
    }
  ]
}
